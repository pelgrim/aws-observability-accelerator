        "x": 8,
        "y": 16
      },
      "id": 3,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": false,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "pluginVersion": "10.4.1",
      "targets": [
        {
          "datasource": {
            "type": "cloudwatch",
            "uid": "${DS_AMAZON_CLOUDWATCH US-EAST-2}"
          },
          "dimensions": {},
          "expression": "fields @timestamp, @message |\n filter message.name like /$Alert/ |\n sort @timestamp desc |\n limit 20",
          "id": "",
          "label": "",
          "logGroups": [
            {
              "arn": "$AMPVendedLog",
              "name": "$AMPVendedLog"
            }
          ],
          "matchExact": true,
          "metricEditorMode": 0,
          "metricName": "",
          "metricQueryType": 0,
